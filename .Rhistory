# Fonction de logging am√©lior√©e
log_debug <- function(message, data = NULL) {
timestamp <- format(Sys.time(), "%Y-%m-%d %H:%M:%S")
cat(sprintf("[DEBUG] %s: %s\n", timestamp, message))
if (!is.null(data)) {
cat("Data structure:\n")
